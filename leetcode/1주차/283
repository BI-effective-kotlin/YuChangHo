https://leetcode.com/problems/move-zeroes/description/

class Solution {
    fun moveZeroes(nums: IntArray): Unit {
        var index = 0;

        for(i in nums.indices) {
            val temp = nums[i]

            nums[index] = if(nums[i] != 0){
                index++
                nums[i] = 0
                temp
            }else {
                0
            }
        }


    }
}
